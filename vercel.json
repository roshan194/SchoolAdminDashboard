{
    "version": 2,
    "buildCommand": "vite build",
    "outputDirectory": "dist/public",
    "installCommand": "npm install",
    "framework": null,
    "functions": {
      "server/index.ts": {
        "runtime": "nodejs18.x"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/index.ts"
      },
      {
        "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/server/index.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }